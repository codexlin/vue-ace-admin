{
  "name": "@codexlin/ace-admin-ui",
  "version": "0.3.0",
  "private": false,
  "description": "Production-ready Vue 3 component library for admin systems. Enhanced Ant Design Vue components with TypeScript support, auto-loading, smart forms, and advanced tables. Works seamlessly with vue-ace-admin",
  "keywords": [
    "vue",
    "vue3",
    "ant-design-vue",
    "antdv",
    "components",
    "ui",
    "typescript",
    "admin",
    "dashboard",
    "pro-components",
    "form",
    "table",
    "business-components",
    "vue-ace-admin",
    "ace-admin"
  ],
  "author": "codexlin",
  "license": "MIT",
  "homepage": "https://github.com/codexlin/vue-ace-admin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codexlin/vue-ace-admin.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/codexlin/vue-ace-admin/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/ace-admin-ui.umd.js",
  "module": "./dist/ace-admin-ui.es.js",
  "style": "./dist/ace-admin-ui.css",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/ace-admin-ui.es.js",
      "require": "./dist/ace-admin-ui.umd.js"
    },
    "./dist/ace-admin-ui.css": {
      "import": "./dist/ace-admin-ui.css",
      "require": "./dist/ace-admin-ui.css"
    },
    "./style.css": {
      "import": "./dist/ace-admin-ui.css",
      "require": "./dist/ace-admin-ui.css"
    },
    "./hooks": {
      "types": "./dist/types/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build",
    "publish:npm": "node scripts/publish.cjs",
    "publish:dry": "node scripts/publish.cjs --dry-run",
    "publish:quick": "node scripts/quick-publish.cjs"
  },
  "peerDependencies": {
    "ant-design-vue": ">=4.0.0",
    "vue": ">=3.4.0",
    "@tinymce/tinymce-vue": "^6.1.0",
    "tinymce": "^7.7.2",
    "tinymce-i18n": "^25.3.17",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "ant-design-vue": "~4.2.6",
    "typescript": "5.5.4",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8",
    "radash": "^12.1.0"
  },
  "dependencies": {
    "@codexlin/ace-admin-hooks": "workspace:*"
  }
}
